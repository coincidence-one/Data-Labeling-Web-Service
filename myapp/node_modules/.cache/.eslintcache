,[],"/Users/ijiyun/Documents/GitHub/Semi-Automatic-AI-Data-Labeling-Platform/myapp/src/modals/ProfileChag.js",["27"],"import React, {useRef, useState} from 'react'\nimport { Modal, Button } from 'react-bootstrap'\nimport '../bootstrap.min.css';\nimport '../App.css';\n\nexport const ProfileChag = ({show, onHide, Image, getData }) => {\n    const fileInput = useRef(null)\n    const [Image2, setImage] = useState(Image)\n    const onChange = (e) => {\n        if(e.target.files[0]){\n             //   setFile(e.target.files[0]) //DB 관련\n            }else{ //업로드 취소할 시\n                setImage(\"https://cdn.pixabay.com/photo/2015/10/05/22/37/blank-profile-picture-973460_1280.png\")\n                return\n            }\n            //화면에 프로필 사진 표시\n           const reader = new FileReader();\n          reader.onload = () => {\n              if(reader.readyState === 2){\n                setImage(reader.result)\n                 getData(reader.result)\n             }\n          }\n         reader.readAsDataURL(e.target.files[0])\n          }\n      return (\n         <Modal\n         show = { show }\n          onHide = { onHide }\n         size=\"lg\"\n         aria-labelledby=\"contained-modal-title-vcenter\"\n         centered\n      >\n        <Modal.Header>\n          <Modal.Title id=\"contained-modal-title-vcenter\">\n            Change Avatar\n          </Modal.Title>\n        </Modal.Header>\n        <Modal.Body>\n          <div style={{textAlign:'center'}}>\n        <img \n        src={Image2} \n        style={{margin:'20px',width:'400px'}} \n        size={200} \n        onClick={()=>{fileInput.current.click()}}\n        />\n        </div>\n       <div className='input_file'>\n        <label className=\"input-file-button\" for=\"input-file\" > {/* label의 for속성을 이용해 커스터마이징 */}\n            Select file \n        </label>\n        <input type=\"file\"  accept='image/jpg,image/png,image/jpeg'   name='profile_img'\n           onChange={onChange}   ref={fileInput} id=\"input-file\" style={{display:\"none\"}}/> \n        </div>\n        \n        </Modal.Body>\n        <Modal.Footer>\n          <Button onClick={onHide} className='close_bt' > Close</Button>\n        </Modal.Footer>\n      </Modal>\n    )\n}\n",["28","29"],{"ruleId":"30","severity":1,"message":"31","line":1,"column":8,"nodeType":"32","messageId":"33","endLine":1,"endColumn":12},{"ruleId":"30","severity":1,"message":"34","line":12,"column":7,"nodeType":"32","messageId":"33","endLine":12,"endColumn":16},{"ruleId":"35","severity":1,"message":"36","line":49,"column":6,"nodeType":"37","endLine":52,"endColumn":9},{"ruleId":"35","severity":1,"message":"36","line":41,"column":9,"nodeType":"37","endLine":46,"endColumn":11},{"ruleId":"38","replacedBy":"39"},{"ruleId":"40","replacedBy":"41"},"no-unused-vars","'logo' is defined but never used.","Identifier","unusedVar","'fileInput' is assigned a value but never used.","jsx-a11y/alt-text","img elements must have an alt prop, either with meaningful text, or an empty string for decorative images.","JSXOpeningElement","no-native-reassign",["42"],"no-negated-in-lhs",["43"],"no-global-assign","no-unsafe-negation"]